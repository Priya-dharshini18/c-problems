#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

    /* Enter your code here. Read input from STDIN. Print output to STDOUT */
    double a,b,c;
    scanf("%lf",&a);
    scanf("%lf",&b);
    scanf("%lf",&c);
    
    double cost=a+b;
    double gain=c-cost;
    double gainp=(gain/cost)*100;
    printf("%.2lf\n",gainp);
    return 0;
}
