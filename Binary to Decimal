#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

    /* Enter your code here. Read input from STDIN. Print output to STDOUT */  
    char binary[100];
    int decimal=0;
    int length,i;
    
    scanf("%s",binary);
    
    length=strlen(binary);
    
    for(i=0;i<length;i++)
    {
        if(binary[i]=='1'){
            decimal=decimal*2+1;
        }
        else if(binary[i]=='0')
        {
            decimal=decimal*2;
        }
        else
        {
            printf("Invalid Input\n");
            return 0;
        }
    }
    printf("%d\n",decimal);
    return 0;
}
