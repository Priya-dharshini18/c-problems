#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

    /* Enter your code here. Read input from STDIN. Print output to STDOUT */   
    int a,b,s=0;
    scanf("%d",&a);
    int n=a;
    while(a>0)
    {
        b=a%10;
        a=a/10;
        s+=b;
    }
    if(n%s==0)
    {
        printf("Harshad Number");
    }
    else
    {
        printf("Not Harshad Number");
    }
    return 0;
}
