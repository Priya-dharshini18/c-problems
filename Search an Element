#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

    /* Enter your code here. Read input from STDIN. Print output to STDOUT */ 
    int n,a,c=0;
    scanf("%d",&n);
    int arr[n];
    for(int i=0; i<n; i++)
    {
        scanf("%d",&arr[i]);
    }
    scanf("%d",&a);
    for(int j=0; j<n; j++)
    {
        if(arr[j]==a)
        {
            c++;
        }
    }
    if(c)
    {
        printf("%d is presented in an array.",a);
        
    }
    else
    { 
    printf("%d is not presented in an array.",a);
    }
    return 0;
}
